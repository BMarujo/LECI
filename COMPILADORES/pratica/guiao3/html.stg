table(header, row) ::= <<
\<table\>
    <header>
    <row>
\</table\>
>>

header(header_values) ::= <<
\<thead\>
    \<tr\>
        <header_values>
    \</tr\>
\</thead\>
>>

header_values(value) ::= <<
\<th\> <value> \</th\>
>>

rows(row_values) ::= <<
\<tr\>
    <row_values>
\</tr\>
>>

row_values(value) ::= <<
\<td\> <value> \</td\>
>>